def shared_pods
    pod 'SVProgressHUD'
    pod 'Alamofire'
    pod 'PromiseKit'
    pod 'SwiftyJSON'
    pod 'DZNEmptyDataSet'
    pod 'Kingfisher'
end

target 'Media Explorer' do
    shared_pods
end


